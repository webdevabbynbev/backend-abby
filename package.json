{
  "name": "backendAPI-AbbynBev",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": {
      "types": "./app/controllers/*.ts",
      "default": "./app/controllers/*.js"
    },
    "#exceptions/*": {
      "types": "./app/exceptions/*.ts",
      "default": "./app/exceptions/*.js"
    },
    "#models/*": {
      "types": "./app/models/*.ts",
      "default": "./app/models/*.js"
    },
    "#mails/*": {
      "types": "./app/mails/*.ts",
      "default": "./app/mails/*.js"
    },
    "#services/*": {
      "types": "./app/services/*.ts",
      "default": "./app/services/*.js"
    },
    "#listeners/*": {
      "types": "./app/listeners/*.ts",
      "default": "./app/listeners/*.js"
    },
    "#events/*": {
      "types": "./app/events/*.ts",
      "default": "./app/events/*.js"
    },
    "#middleware/*": {
      "types": "./app/middleware/*.ts",
      "default": "./app/middleware/*.js"
    },
    "#validators/*": {
      "types": "./app/validators/*.ts",
      "default": "./app/validators/*.js"
    },
    "#providers/*": {
      "types": "./providers/*.ts",
      "default": "./providers/*.js"
    },
    "#policies/*": {
      "types": "./app/policies/*.ts",
      "default": "./app/policies/*.js"
    },
    "#abilities/*": {
      "types": "./app/abilities/*.ts",
      "default": "./app/abilities/*.js"
    },
    "#database/*": {
      "types": "./database/*.ts",
      "default": "./database/*.js"
    },
    "#start/*": {
      "types": "./start/*.ts",
      "default": "./start/*.js"
    },
    "#tests/*": {
      "types": "./tests/*.ts",
      "default": "./tests/*.js"
    },
    "#config/*": {
      "types": "./config/*.ts",
      "default": "./config/*.js"
    },
    "#utils/*": {
      "types": "./app/utils/*.ts",
      "default": "./app/utils/*.js"
    },
    "#enums/*": {
      "types": "./app/enums/*.ts",
      "default": "./app/enums/*.js"
    }
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
