    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/lodash": "^4.17.20",
    "@vinejs/vine": "^3.0.1",
    "adonis-autoswagger": "^3.73.0",
    "adonis-captcha-guard": "^1.0.1",
    "axios": "^1.13.2",
    "cloudinary": "^2.8.0",
    "csv-parser": "^3.2.0",
    "edge.js": "^6.3.0",
    "exceljs": "^4.4.0",
    "google-auth-library": "^10.2.1",
    "jsbarcode": "^3.12.1",
    "luxon": "^3.7.1",
    "midtrans-client": "^1.4.3",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "openai": "^5.23.2",
    "reflect-metadata": "^0.2.2",
    "rollup": "^4.54.0",
    "swagger-ui-express": "^5.0.1"
